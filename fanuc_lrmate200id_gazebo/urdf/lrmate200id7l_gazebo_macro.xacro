<?xml version = "1.0" ?>
<robot name = "fanuc_lrmate200id7l" xmlns:xacro="http://www.ros.org/wiki/xacro" >
<xacro:macro name="fanuc_lrmate200id7l" params="prefix">


<!--Gazebo ROS Control -->
<gazebo>
  <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    <robotNamespace>/${prefix}</robotNamespace>
    <legacyModeNS>true</legacyModeNS>
  </plugin>
</gazebo>

<!--Base -->


<link name="${prefix}base_link">
	<collision>
		<geometry>
			<mesh filename = "package://fanuc_lrmate200id_support/meshes/lrmate200id/collision/base_link.stl"  />
		</geometry>
	<origin xyz = "0 0 0" rpy = "0 0 0"/><!-- Best value <origin xyz = "0.105 0.09 0" rpy = "0 0 0" />-->
	</collision>

	<visual>
 		<geometry>
			<mesh filename = "package://fanuc_lrmate200id_support/meshes/lrmate200id/visual/base_link.stl" />
		</geometry>
	<origin xyz = "0 0 0" rpy = "0 0 0"/><!-- Best value <origin xyz = "0.105 0.09 0" rpy = "0 0 0" />-->
	</visual>
	<inertial>
		<origin xyz = "0.105 0.09 0.08042" rpy = "0 0 0" /> <!-- Best value <origin xyz = "0.105 0.09 0.08042" rpy = "0 0 0" />-->
		<mass value = "4.85331"/>
		<inertia ixx ="0.000533781" iyy=".055445798" izz=".02302818052" ixy="0.00012915752" ixz ="-0.002807595" iyz = "-0.000094967"/>
	</inertial>
</link>

<link name = "world"/>

<joint name="world_base_joint" type="fixed">
	<parent link="world"/>
	<child link="${prefix}base_link" />
	<origin xyz = "0 0 0" rpy = "0 0 0"/>
</joint> 

<gazebo reference= "${prefix}base_link" >
	<material>Gazebo/Black</material>
</gazebo>

<joint name="${prefix}joint_1" type="revolute">
      <origin xyz="0 0 0.330" rpy="0 0 0" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}link_1" />
      <axis xyz="0 0 1" />
      <limit effort="0" lower="-2.965" upper="2.965" velocity="7.85" />
</joint>

<!-- Part 2 -->
